<h1>Ironhack Cinema</h1>
<p>Click on the movies to check the showtimes</p>
<div class="allmovie">
{{#each movies}}
<div class="eachmovie">
    <div> <img src={{this.image}} /> </div>  
    <div class="movietitle">{{this.title}}</div>
    <div class="seemore"> <a href="/movies/{{_id}}">See more</a> </div>
</div>
{{/each}}
</div>